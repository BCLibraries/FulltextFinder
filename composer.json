{
  "name": "bclibraries/fulltext-finder",
  "description": "Finds links to fulltexts from citations",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Florin",
      "email": "benjamin.florin@bc.edu"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "7.1.26",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "symfony/http-client": "^4.3",
    "nyholm/psr7": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codedungeon/phpunit-result-printer": "0.25.1",
    "php-http/mock-client": "^1.3",
    "mockery/mockery": "^1.2"
  },
  "config": {
    "platform": {
      "php": "7.1.26"
    }
  },
  "autoload": {
    "psr-4": {
      "BCLib\\FulltextFinder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BCLib\\FulltextFinder\\Tests\\": "tests/"
    }
  }
}
