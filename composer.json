{
  "name": "bclibraries/fulltext-finder",
  "description": "Finds links to fulltexts from citations",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Florin",
      "email": "benjamin.florin@bc.edu"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "symfony/http-client": "^5.1",
    "nyholm/psr7": "^1.0",
    "bclibraries/libkey-client": "^v0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "codedungeon/phpunit-result-printer": "0.26.1",
    "php-http/mock-client": "^1.4.1",
    "mockery/mockery": "^1.4.2"
  },
  "config": {
    "platform": {
      "php": "7.4.12"
    }
  },
  "autoload": {
    "psr-4": {
      "BCLib\\FulltextFinder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BCLib\\FulltextFinder\\Tests\\": "tests/"
    }
  }
}
